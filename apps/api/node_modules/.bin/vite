#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Work/my-monorepo/node_modules/.pnpm/vite@7.3.1_@types+node@20.1_23d27ba4b9e3c0d514511fcbfa0443fa/node_modules/vite/bin/node_modules:/mnt/e/Work/my-monorepo/node_modules/.pnpm/vite@7.3.1_@types+node@20.1_23d27ba4b9e3c0d514511fcbfa0443fa/node_modules/vite/node_modules:/mnt/e/Work/my-monorepo/node_modules/.pnpm/vite@7.3.1_@types+node@20.1_23d27ba4b9e3c0d514511fcbfa0443fa/node_modules:/mnt/e/Work/my-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Work/my-monorepo/node_modules/.pnpm/vite@7.3.1_@types+node@20.1_23d27ba4b9e3c0d514511fcbfa0443fa/node_modules/vite/bin/node_modules:/mnt/e/Work/my-monorepo/node_modules/.pnpm/vite@7.3.1_@types+node@20.1_23d27ba4b9e3c0d514511fcbfa0443fa/node_modules/vite/node_modules:/mnt/e/Work/my-monorepo/node_modules/.pnpm/vite@7.3.1_@types+node@20.1_23d27ba4b9e3c0d514511fcbfa0443fa/node_modules:/mnt/e/Work/my-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@7.3.1_@types+node@20.1_23d27ba4b9e3c0d514511fcbfa0443fa/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@7.3.1_@types+node@20.1_23d27ba4b9e3c0d514511fcbfa0443fa/node_modules/vite/bin/vite.js" "$@"
fi
